{
    "version": 2,
    "builds": [
        {
            "src": "server.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "public/(.*)",
            "dest": "public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server.mjs"
        }
    ]
}