{
    "version": 2,
    "builds": [
        {
            "src": "server.mjs",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.mjs"
        }
    ],
    "static": {
        "source": "public",
        "basePath": "/public"
    }
}